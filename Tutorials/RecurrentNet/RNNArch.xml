<?xml version="1.0"?>
<data>
    <layer type="Data" name="data1">
        <input>input</input>
        <batchsize>128</batchsize>
        <datasize>136</datasize>
   </layer>
    <layer type="LU" name="fc1">
        <input>data1</input>
        <numunits>128</numunits>
        <CloneCoeff>1.0</CloneCoeff>
        <bias>1.0</bias>
    </layer>
    <layer type="Rectifier" name="rct1">
        <input>fc1</input>
        <posslope>
            <min>0.0</min>
            <max>1.0</max>
            <init>1.0</init>
            <method>Constant</method>
        </posslope>
        <negslope>
            <min>0.0</min>
            <max>1.0</max>
            <init>0.</init>
            <method>Constant</method>
        </negslope>
    </layer>
    <layer type="Concatenate" name="concat">
        <input>rct1</input>
        <recurrent_input>rct1</recurrent_input>
        <recurrent_input>rct2</recurrent_input>
        <recurrent_input>fc3</recurrent_input>
    </layer>
    <layer type="LU" name="fc2">
        <input>concat</input>
        <numunits>128</numunits>
        <CloneCoeff>1.0</CloneCoeff>
        <bias>1.0</bias>
    </layer>
    <layer type="Rectifier" name="rct2" feedback_output="yes">
        <input>fc2</input>
        <posslope>
            <min>0.0</min>
            <max>1.0</max>
            <init>1.0</init>
            <method>Constant</method>
        </posslope>
        <negslope>
            <min>0.0</min>
            <max>1.0</max>
            <init>0.</init>
            <method>Constant</method>
        </negslope>
    </layer>
    <layer type="LU" name="fc3" feedback_output="yes">
        <input>rct2</input>
        <numunits>10</numunits>
        <CloneCoeff>1.0</CloneCoeff>
        <bias>1.0</bias>
    </layer>
    <layer type="ActionSelection" name="act1">
        <input>fc3</input>
        <type>soft</type>
    </layer>
</data>
